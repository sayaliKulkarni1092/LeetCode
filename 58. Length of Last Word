class Solution {
    public int lengthOfLastWord(String s) {
        int result = 0;

        if (s != null && s.trim().length() != 0) {
            String[] sArr = s.trim().split(" ");

            String lastWord = sArr[sArr.length - 1];

            result = lastWord.length();

        }



        return result;

    
    }
}
