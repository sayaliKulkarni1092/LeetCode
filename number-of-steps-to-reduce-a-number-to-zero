class Solution {
    public int numberOfSteps(int num) {
        int result = 0;
        while(num > 0) {
            if (num%2 == 0) {
                num = num / 2;
            } else {
                num -= 1;
            }
            result++;
        }
        

        return result;
    }

    // time complexity O(log n) as time is depend on divisible
    //space complexity O(1)
}
